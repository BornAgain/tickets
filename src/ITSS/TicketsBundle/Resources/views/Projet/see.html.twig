{% extends "::layout.html.twig" %}

{% block pageheader %}
    Project Management
    <small>View project</small>
{% endblock %}

{% block content %}
    <div class="box box-primary">
        <div class="box-header">
            <h3 class="box-title">Project Form</h3>
        </div><!-- /.box-header -->
        <div class="box-body">
            <div class="col-md-6" >
                <div class="form-group">
                    <label for="name">Name</label>
                    <input type="name" class="form-control" id="exampleInputName" value="{{ entity.name }}" disabled>                    
                </div>
                <div class="form-group">
                    <label for="dateBegin">Start Date</label>
                    <input type="dateBegin" class="form-control" id="exampleInputDateBegin" value="{{ entity.dateBegin|date("Y/m/d") }}" disabled>
                </div>
                <div class="form-group">
                    <label for="dateEnd">End Date</label>
                    <input type="dateEnd" class="form-control" id="exampleInputDateEnd" value="{{ entity.dateEnd|date("Y/m/d") }}" disabled>
                </div>
            </div>
            <div class="col-md-6" >
                <div class="form-group">
                    <label for="statut">Statut</label>
                    <input type="statut" class="form-control" id="exampleInputStatut" value="{{ entity.statut }}" disabled>
                </div>
                <div class="form-group">
                    <label for="client">Client</label>
                    <input type="client" class="form-control" id="exampleInputClient" value="{{ entity.client }}" disabled>
                </div>
                <div class="form-group">
                    <label for="product">Product</label>
                    <input type="product" class="form-control" id="exampleInputPoduct" value="{{ entity.product }}" disabled>
                </div>               
            </div>
        </div><!-- /.box-body -->

        <div class="box-footer">
        </div>
    </div><!-- /.box -->
{% endblock %}
